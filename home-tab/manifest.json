{
	"name": "Home Tab",
	"version": "1",
	"manifest_version": 2,

	"description": "Configure a home page for new tabs.",
	"developer": {
        "name": "Gabriel",
        "url": "https://github.com/gquagliano/extension-opera-pagina-inicio"
    },
	"icons": {
        "16": "iconos/icono_16.png",
        "19": "iconos/icono_19.png",
        "48": "iconos/icono_48.png",
        "128": "iconos/icono_128.png"
    },
    "permissions": [
        "tabs",
        "contextMenus"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "options_page": "configuracion.html"
}